{
  "name": "react-native-inappstory-sdk-example",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "start": "react-native start",
    "build:android": "cd android && ./gradlew assembleDebug --no-daemon --console=plain -PreactNativeArchitectures=arm64-v8a",
    "build:ios": "cd ios && xcodebuild -workspace InappstorySdkExample.xcworkspace -scheme InappstorySdkExample -configuration Debug -sdk iphonesimulator CC=clang CPLUSPLUS=clang++ LD=clang LDPLUSPLUS=clang++ GCC_OPTIMIZATION_LEVEL=0 GCC_PRECOMPILE_PREFIX_HEADER=YES ASSETCATALOG_COMPILER_OPTIMIZATION=time DEBUG_INFORMATION_FORMAT=dwarf COMPILER_INDEX_STORE_ENABLE=NO"
  },
  "dependencies": {
    "@devvie/bottom-sheet": "^0.3.0",
    "@miblanchard/react-native-slider": "^2.6.0",
    "@react-native-picker/picker": "^2.7.7",
    "@react-navigation/native": "^6.1.17",
    "@react-navigation/native-stack": "^6.9.26",
    "react": "18.2.0",
    "react-content-loader": "^7.0.0",
    "react-native": "0.74.2",
    "react-native-button": "^3.1.0",
    "react-native-device-info": "^11.1.0",
    "react-native-dropdown-picker": "^5.4.6",
    "react-native-element-dropdown": "^2.12.0",
    "react-native-gesture-handler": "^2.16.2",
    "react-native-get-random-values": "^1.11.0",
    "react-native-network-logger": "^1.16.1",
    "react-native-reanimated": "^3.12.0",
    "react-native-safe-area-context": "^4.10.5",
    "react-native-screens": "^3.30.1",
    "react-native-select-dropdown": "^4.0.1",
    "react-native-share": "^10.2.1",
    "react-native-simple-toast": "^3.3.1",
    "react-native-svg": "^15.3.0",
    "react-native-video": "^6.2.0",
    "react-native-vision-camera": "^4.3.2",
    "react-native-wheel-color-picker": "^1.3.1",
    "rn-android-keyboard-adjust": "^2.1.2",
    "zustand": "^4.5.2"
  },
  "devDependencies": {
    "@babel/core": "^7.24.7",
    "@babel/preset-env": "^7.20.0",
    "@babel/runtime": "^7.20.0",
    "@react-native/babel-preset": "0.74.81",
    "@react-native/metro-config": "0.74.81",
    "@react-native/typescript-config": "0.74.84",
    "@types/react-native-button": "^3.0.5",
    "@welldone-software/why-did-you-render": "^8.0.3",
    "babel-plugin-module-resolver": "^5.0.2"
  },
  "engines": {
    "node": ">=18"
  }
}
